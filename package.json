{
  "name": "nation-navigator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ip": "ng serve --port 8080 --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:ssr": "ng build --c=production",
    "serve:ssr": "node dist/nation-navigator/server/server.mjs",
    "prerender": "ng run nation-navigator:prerender",
    "icons": "svg-to-ts-files svg-to-ts"
  },
  "private": true,
  "svg-to-ts": {
    "srcFiles": [
      "./src/assets/icons/*.svg",
      "./src/assets/icons/extra/*.svg"
    ],
    "outputDirectory": "./src/assets/svg-icon",
    "interfaceName": "TmdIcon",
    "typeName": "ng",
    "prefix": "ng",
    "delimiter": "KEBAB",
    "svgoConfig": {
      "plugins": [
        "cleanupAttrs"
      ]
    },
    "fileName": "tmd-icon.model",
    "compileSources": true
  },
  "dependencies": {
    "@angular/animations": "^18.0.1",
    "@angular/common": "^18.0.1",
    "@angular/compiler": "^18.0.1",
    "@angular/core": "^18.0.1",
    "@angular/forms": "^18.0.1",
    "@angular/platform-browser": "^18.0.1",
    "@angular/platform-browser-dynamic": "^18.0.1",
    "@angular/platform-server": "^18.0.1",
    "@angular/router": "^18.0.1",
    "@angular/ssr": "^18.0.2",
    "bootstrap": "^5.3.3",
    "express": "^4.18.2",
    "javascript": "^1.0.0",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.0",
    "svg-to-ts": "^12.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.2",
    "@angular/cli": "^18.0.2",
    "@angular/compiler-cli": "^18.0.1",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.5"
  }
}
